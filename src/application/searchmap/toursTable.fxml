<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="50.0" text="Back" />
    <TableView fx:id="toursTable" layoutX="23.0" layoutY="84.0" onMouseClicked="#userClickOnTable" prefHeight="131.0" prefWidth="256.0">
      <columns>
        <TableColumn prefWidth="75.0" text="TourID" fx:id="tourIDColumn" />
        <TableColumn prefWidth="180.0" text="#Attractions" fx:id="numAttractionsColumn" />
      </columns>
    </TableView>
    <Label fx:id="title" layoutX="92.0" layoutY="29.0" prefHeight="21.0" prefWidth="146.0" text="City Tours">
      <font>
        <Font name="System Bold" size="22.0" />
      </font>
    </Label>
    <Button fx:id="tourInfoBtn" layoutX="118.0" layoutY="335.0" mnemonicParsing="false" onAction="#tourInfo" text="Tour Info" />
    <Button fx:id="addTourBtn" layoutX="23.0" layoutY="335.0" mnemonicParsing="false" onAction="#addTour" prefHeight="25.0" prefWidth="89.0" text="Add Tour" />
    <Button fx:id="removeTourBtn" layoutX="193.0" layoutY="335.0" mnemonicParsing="false" onAction="#removeTour" prefHeight="25.0" prefWidth="89.0" text="Remove Tour" />
    <Label layoutX="15.0" layoutY="226.0" text="Tour Description:" />
    <TextArea id="cityDescription" fx:id="tourDescription" layoutX="29.0" layoutY="254.0" prefHeight="60.0" prefWidth="256.0" wrapText="true" />
  </children>
</AnchorPane>
