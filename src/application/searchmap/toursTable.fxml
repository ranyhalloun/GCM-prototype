<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="50.0" text="Back" />
      <TableView fx:id="toursTable" layoutX="23.0" layoutY="100.0" onMouseClicked="#userClickOnTable" prefHeight="200.0" prefWidth="256.0">
        <columns>
          <TableColumn fx:id="tourIDColumn" prefWidth="75.0" text="TourID" />
          <TableColumn fx:id="descriptionColumn" prefWidth="180.0" text="Description" />
        </columns>
      </TableView>
      <Label fx:id="title" layoutX="92.0" layoutY="29.0" prefHeight="21.0" prefWidth="146.0" text="Label">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="tourInfoBtn" layoutX="118.0" layoutY="319.0" mnemonicParsing="false" onAction="#tourInfo" text="Tour Info" />
      <Button fx:id="addTourBtn" layoutX="23.0" layoutY="319.0" mnemonicParsing="false" onAction="#addTour" prefHeight="25.0" prefWidth="89.0" text="Add Tour" />
      <Button fx:id="removeTourBtn" layoutX="193.0" layoutY="319.0" mnemonicParsing="false" onAction="#removeTour" prefHeight="25.0" prefWidth="89.0" text="Remove Tour" />
   </children>
</AnchorPane>
