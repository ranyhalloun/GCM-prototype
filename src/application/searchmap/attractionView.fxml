<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <TableView id="attractionsTable" fx:id="attractionsTableView" layoutX="26.0" layoutY="98.0" onMouseClicked="#userClickOnTable" prefHeight="134.0" prefWidth="108.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="111.0" text="City Name" fx:id="nameColumn" />
      </columns>
    </TableView>
    <Label layoutX="96.0" layoutY="17.0" prefHeight="32.0" prefWidth="108.0" text="Attractions">
      <font>
        <Font name="System Bold" size="18.0" />
      </font>
    </Label>
    <Button id="addBtn" fx:id="goToMapsBtn" layoutX="55.0" layoutY="346.0" mnemonicParsing="false" onAction="#goToMaps" prefHeight="32.0" prefWidth="191.0" text="Continue To Maps" />
    <TextArea id="descriptionFT" fx:id="description" layoutX="39.0" layoutY="275.0" prefHeight="60.0" prefWidth="225.0" />
    <Label layoutX="15.0" layoutY="244.0" text="Description:">
      <font>
        <Font size="14.0" fx:id="x1" />
      </font>
    </Label>
    <Button id="backBtn" fx:id="back" layoutX="15.0" layoutY="17.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="47.0" text="Back" />
    <Label font="$x1" layoutX="141.0" layoutY="98.0" text="Number of maps include" />
    <Label font="$x1" layoutX="141.0" layoutY="114.0" text="this attraction:" />
    <TextArea fx:id="numOfMapIncludeAttraction" layoutX="150.0" layoutY="149.0" prefHeight="32.0" prefWidth="108.0" wrapText="true" />
    <Label font="$x1" layoutX="26.0" layoutY="57.0" text="Cities that include" />
    <Label font="$x1" layoutX="26.0" layoutY="72.0" text="the attraction:" />
  </children>
</AnchorPane>
