<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="Back" />
    <TableView fx:id="tourTable" layoutX="44.0" layoutY="64.0" onMouseClicked="#userClickOnTable" prefHeight="157.0" prefWidth="219.0">
      <columns>
        <TableColumn prefWidth="114.0" text="Attraction Name" fx:id="attractionNameColumn" />
        <TableColumn prefWidth="104.0" text="Time" fx:id="timeColumn" />
      </columns>
    </TableView>
    <Label layoutX="102.0" layoutY="18.0" prefHeight="21.0" prefWidth="124.0" text="Tour Attractions">
      <font>
        <Font name="System Bold" size="15.0" />
      </font>
    </Label>
    <Button fx:id="addBtn" layoutX="70.0" layoutY="341.0" mnemonicParsing="false" onAction="#add" prefHeight="25.0" prefWidth="59.0" text="Add" />
    <Button fx:id="removeBtn" layoutX="161.0" layoutY="341.0" mnemonicParsing="false" onAction="#remove" text="Remove" />
    <TextArea id="descriptionFT" fx:id="attractionDescription" layoutX="38.0" layoutY="266.0" prefHeight="60.0" prefWidth="225.0" />
    <Label layoutX="17.0" layoutY="234.0" text="Attraction Description:">
      <font>
        <Font size="14.0" />
      </font>
    </Label>
  </children>
</AnchorPane>
