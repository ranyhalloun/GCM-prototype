<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="104.0" layoutY="36.0" text="New Prices">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <GridPane layoutX="14.0" layoutY="110.0" prefHeight="63.0" prefWidth="283.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="111.0" minWidth="10.0" prefWidth="111.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="79.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.0" minWidth="10.0" prefWidth="93.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="31.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="32.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Subscription Price">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <Label fx:id="oldSubs" prefHeight="17.0" prefWidth="77.0" GridPane.columnIndex="1" />
            <Label text="OneTime Price" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold Italic" size="12.0" />
               </font>
            </Label>
            <Label fx:id="oldOne" prefHeight="17.0" prefWidth="77.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="newSubs" prefHeight="17.0" prefWidth="98.0" GridPane.columnIndex="2" />
            <Label fx:id="newOne" prefHeight="17.0" prefWidth="91.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="acceptBtn" layoutX="61.0" layoutY="248.0" mnemonicParsing="false" onAction="#accept" text="Accept" />
      <Button fx:id="declineBtn" layoutX="177.0" layoutY="248.0" mnemonicParsing="false" onAction="#decline" text="Decline" />
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="53.0" text="Back" />
      <Label layoutX="136.0" layoutY="93.0" text="OLD" />
      <Label layoutX="219.0" layoutY="93.0" text="NEW" />
   </children>
</AnchorPane>
